{
  "manifest_version": 2,

  "name": "Tarantula Hotkeys",
  "description": "This extension enables hotkeys for tarantula",
  "version": "0.32",

  "permissions": [
    "tabs"
  ],

  "icons": {
    "16": "16.png",
    "32": "32.png",
    "64": "64.png"
  },

  "background": { "scripts": ["background.js"] },

  "browser_action": {
    "default_icon": "32.png"
  },

  "content_scripts": [
    {
    "js": ["jquery-1.12.3.min.js", "hotkeys.js"],
    "matches": [ "http://10.17.22.220/*"]
    }
  ]
}
